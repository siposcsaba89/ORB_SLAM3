#laod dbow targets
include(CMakeFindDependencyMacro)
find_dependency(OpenCV @OpenCV_VERSION@ EXACT)
find_dependency(Eigen3 3.1.0 REQUIRED CONFIG)
find_dependency(Pangolin REQUIRED CONFIG)
find_dependency(g2o CONFIG REQUIRED)
find_dependency(DBoW2 REQUIRED)
#prefer exported target instead of old cmake variables
if (TARGET DBoW2::DBoW2)
	set(dbow2_LIBRARIES DBoW2::DBoW2)
else()
	set(dbow2_LIBRARIES ${DBoW2_LIBRARIES})
endif()

set(Boost_USE_STATIC_LIBS        OFF) # only find static libs
set(Boost_USE_MULTITHREADED      ON)
set(Boost_USE_STATIC_RUNTIME    OFF)
find_package(Boost REQUIRED COMPONENTS serialization)
include(${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-targets.cmake)
